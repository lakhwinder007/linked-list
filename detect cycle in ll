bool detectLoop(Node* head)
    {
        // your code here
        if(head==NULL||head->next==NULL)return false;
        Node* slow=head,*fast =head;
        while(fast!=NULL&&fast->next!=NULL){

            fast=fast->next->next;
            slow=slow->next;
            if(fast==slow)return true;
        }
        return false;
    }
